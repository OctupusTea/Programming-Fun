<Window x:Name="Paint_Test" x:Class="Paint_Test.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Paint_Test"
        mc:Ignorable="d"
        Title="Paint Test" Height="450" Width="800">
    <Grid>
        <Canvas x:Name="PaintBoard" Margin="143,10,10,10" MouseDown="PaintBoard_MouseDown" MouseMove="PaintBoard_MouseMove" Background="#FFFFFFC0" Focusable="True" MouseUp="PaintBoard_MouseUp"/>
        <Rectangle Fill="#FFF4F4F5" HorizontalAlignment="Left" Margin="10,10,0,10" Stroke="Black" Width="128"/>
        <Button x:Name="UndoButton" Content="Undo(Z)" VerticalAlignment="Top" Height="45" FontSize="16" Click="UndoButton_Click" IsEnabled="False" Margin="20,21,0,0" HorizontalAlignment="Left" Width="108"/>
        <Button x:Name="RedoButton" Content="Redo(Y)" VerticalAlignment="Top" Height="45" FontSize="16" Click="RedoButton_Click" IsEnabled="False" Margin="20,71,0,0" HorizontalAlignment="Left" Width="108"/>

    </Grid>
</Window>
